{
  "breadcrumbs.enabled": true,
  "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
  "C_Cpp.intelliSenseEngine": "Default",
  "c-cpp-flylint.flexelint.enable": false,
  "editor.fontFamily": "Source Code Pro, Ricty Diminished",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [120],
  "editor.scrollBeyondLastLine": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "explorer.openEditors.visible": 0,
  "files.associations": {
    ".Brewfile": "shellscript",
    ".clang-format": "yaml",
    ".envrc": "shellscript"
  },
  "files.watcherExclude": {
    "**/.direnv/**": true
  },
  "git.autofetch": true,
  "gitProjectManager.baseProjectsFolders": ["~/src"],
  "gitProjectManager.openInNewWindow": true,
  "gitProjectManager.storeRepositoriesBetweenSessions": true,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "python.formatting.provider": "black",
  "python.pythonPath": "python3",
  "ruby.lint": {
    "rubocop": true,
    "ruby": true
  },
  "rust-client.channel": "nightly",
  "rust.cfg_test": true,
  "search.exclude": {
    "**/.direnv": true
  },
  "vetur.format.defaultFormatter.html": "prettyhtml",
  "vetur.format.defaultFormatterOptions": {
    "prettyhtml": {
      "printWidth": 120
    }
  },
  "window.title": "[${rootName}] ${activeEditorMedium} ${dirty}",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none"
}
