{
  "breadcrumbs.enabled": true,
  "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
  "C_Cpp.intelliSenseEngine": "Default",
  "c-cpp-flylint.flexelint.enable": false,
  "editor.fontFamily": "Source Code Pro, Ricty Diminished",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.scrollBeyondLastLine": false,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    ".Brewfile": "shellscript",
    ".clang-format": "yaml",
    ".envrc": "shellscript"
  },
  "git.autofetch": true,
  "gitProjectManager.baseProjectsFolders": ["~/src"],
  "gitProjectManager.openInNewWindow": true,
  "gitProjectManager.storeRepositoriesBetweenSessions": true,
  "prettier.singleQuote": true,
  "python.pythonPath": "python3",
  "ruby.lint": {
    "rubocop": true,
    "ruby": true
  },
  "rust-client.channel": "nightly",
  "rust.cfg_test": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Tomorrow Night",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none"
}
